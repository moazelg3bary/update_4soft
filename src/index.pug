                li.icons_li(data-src="../image/html-icon.svg", data-linkView="html", id="../image/html-icon.svg", data-src2="../image/html-g.svg")
                  img(src="../image/html-g.svg", alt="html-icon")
            // some class flexbox B4
            .views.active.flex-wrap.justify-content-center#view
              .col-sm-12.col-md-4
                .view
